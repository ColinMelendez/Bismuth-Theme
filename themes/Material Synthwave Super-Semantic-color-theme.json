	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#676e95ae", // -- primary 0 -- comment - trans grey
				"fontStyle": "italic"
			}
		},
		{
			"name": "Colors", // ??
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFF", // -- editor 2 -- important text - pure white
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370", // -- primary 3 -- function parameters - bright red
			}
		},
		{
			"name": "function parameter variables in signature and body",
			"scope": [
				"variable.parameter.function.language.special",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#73e8b3" // -- primary 11 -- function parameters - bright mint
			}
		},
		{
			"name": "Invalid.depreciated (mark for depreciated features)",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#C792EA", // -- primary 6 -- keywords & symbols - cs lavender
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Primary keywords",
			"scope": [
				// "keyword", // this overrides too many things as a semantic token.
				"storage.type",
				"storage.modifier",
				"keyword.other.rust",
				"keyword.operator.math",
				"keyword.operator.namespace",
				"keyword.operator.key-value",
				"keyword.operator.expression.in",
				"keyword.operator.type",
				"keyword.operator.expression.keyof",
				"keyword.operator.comparison",
				"keyword.operator.expression.instanceof",
				"keyword.operator.arithmetic",
				"keyword.operator.pipe",
				"support.type.property-name.table.toml",
				"meta.attribute.rust",
				"keyword.operator.relation bal.js",
				"keyword.operator.relational",
				"keyword.operator.expression.void",
				"keyword.operator.expression.typeof",
				"keyword.operator.expression.of",
				"keyword.operator.decrement",
				"keyword.operator.increment",
				"keyword.generator.asterisk",
				"meta.at-rule",
				"keyword.package",
				"keyword.var",
				"keyword.function",
				"keyword.type",
				"keyword.struct",
				"keyword.channel",
				"keyword.const",
			],
			"settings": {
				"foreground": "#C792EA", // -- primary 6 -- keywords & symbols - cs lavender
				"fontStyle": ""
			}
		},
		{
			"name": "cover styles",
			"scope": [
				"meta.attribute.rust",
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "decorators (macro stuff)",
			"scope": [
				"entity.name.decorator",
			],
			"settings": {
				"foreground": "#C792EA", // -- primary 6 -- keywords & symbols - cs lavender
				"fontStyle": "underline"
			}
		},
		{
			"name": "Operators, Punctuation, html brackets, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator.access",
				"punctuation.definition.table.inline.toml",
				"punctuation.separator.table.inline.toml",
				"punctuation.eq.toml",
				"keyword.operator.macro.dollar.rust",
				"meta.brace.curly.js",
				"keyword.operator.accessor.js",
				"meta.delimiter.comma.js",
				"meta.brace.round.js",
				"meta.function.js",
				"meta.method-call.with-arguments.js",
				"keyword.operator.range",
				"keyword.operator.ternary",
				"keyword.operator.spread",

			],
			"settings": {
				// alts: "#89DDFF" (less saturation)
				//		 "#58e2eb" (more saturation)
				"foreground": "#89DDFF", // -- primary 5 -- operators & punctuation  - turquoise blue
			}
		},
		{
			"name": "control-flow operators",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"fontStyle": "italic" // control flow operators are italic
			}
		},
		{
			// specially called-out keywords for assignments and generation
			"name": "special operators - assignment",
			"scope": [
				"keyword.operator.assignment",
				"storage.type.function.arrow",
				"keyword.operator.new",
				"keyword.operator.arrow",
				"keyword.operator.arrow.skinny",
				"keyword.operator.arrow.fat",
				"keyword.operator.borrow", // rust borrow symbol
				"keyword.operator.optional",// "?" non-null unwrap in most langs (like js)
				"keyword.operator.question", // "?" non-null unwrap in rust
				"keyword.operator.dereference", // "*" pointer dereference
				"keyword.operator.existential",
				"keyword.operator.nullcoalesce",
				"keyword.operator.address",
				"keyword.operator.channel",
			],
			"settings": {
				"foreground": "#d84bff", // special color - radioactive purple
				// "fontStyle": "italic bold"
			}
		},
		{
			// logical operators (and, or, not)
			// does not include comparison operators (==, !=, <, >, etc.)
			// Logical operators are relevant to specially call-out because
			// they often have specific secondary meanings in different languages
			// (like undefined assertions or bitwise operations), and also because
			// the singular ! operator is difficult too see
			"name": "special operators - logical",
			"scope": [
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#ac71ff", // special color - slightly stronger keyword purple
				"fontStyle": "bold"
			}
		},
		{
			"name": "identifiers in import statements",
			"scope": [
				"meta.import variable.other.readwrite.alias",
				// "selfKeyword" // was always being overridden
				"variable.other.enummember",
			],
			"settings": {
				"foreground": "#e8708a",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Tag",
			"scope": [
				// immutable properties of objects (sorta their own domain of literals)
				"support.constant.property",
				// moved the html / jsx tag color to separate it from local constants visually
				"entity.name.tag", // a1
				"meta.tag.sgml", // a2
				"markup.deleted.git_gutter", // a3
				// react/solid components as types for JSX tags (font modified below)
				"support.class.component", // a4
				// special global constants that use a convention of being all-caps (like rust)
				"constant.other.caps", // a5
			],
			"settings": {
				"foreground": "#ea7790", // -- primary 7 -- tags & immutable variables -  light coral red
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				// react/solid components as types for JSX tags (inherits color from above)
				"support.class.component", // a4
			],
			"settings": {
				"fontStyle": "underline" // no italic
			}
		},
		{
			"scope": [
				// moved the html / jsx tag color to separate it from local constants visually
				"entity.name.tag", // a1
				"meta.tag.sgml", // a2
				"markup.deleted.git_gutter", // a3
			],
			"settings": {
				"fontStyle": "italic underline" // both italic and underline
			}
		},
		{
			"scope": [
				// global constants that use a convention of being all-caps (like rust)
				"constant.other.caps", // a5
			],
			"settings": {
				"fontStyle": "" // none - clears inherited fontStyle
			}
		},
		{
			"scope": [
				// Text literals inside of HTML and JSX blocks
				"meta.jsx.children",
				"text.html.derivative",
			],
			"settings": {
				"foreground": "#f0feff", // special - white
			}
		},
		{
			"name": "Functions, Methods",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"keyword.other.special-method",
				"meta.function-call.generic",
				"meta.selector.css",
				"entity.name.function.tsx",
				"entity.name.function.rust",
				"method",
				"support.function",
				"meta.property-value.scss",
			],
			"settings": {
				"foreground": "#82AAFF", // -- primary 4 -- functions & methods - sky blue
			}
		},
